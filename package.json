{
	"name": "cyberrange",
	"version": "0.0.3",
	"description": "",
	"main": "src/index.js",
	"scripts": {
		"start": "tsc && node ./src/index.js",
		"fix": "eslint --fix src/**/*.ts && prettier --write src/**/*.ts",
		"build": "tsc",
		"lint": "eslint src/**/*.ts && prettier --check src/**/*.ts",
		"publish": "npm run fix && npm run build && npm publish"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/matteopolak/cyberrange.git"
	},
	"author": "Matteo Polak (matteopolak@hotmail.com)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/matteopolak/cyberrange/issues"
	},
	"homepage": "https://github.com/matteopolak/cyberrange#readme",
	"importSort": {
		".js, .jsx, .ts, .tsx": {
			"style": "module",
			"parser": "typescript"
		}
	},
	"devDependencies": {
		"@types/node": "^17.0.33",
		"@typescript-eslint/eslint-plugin": "^5.23.0",
		"@typescript-eslint/parser": "^5.23.0",
		"eslint": "^8.15.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"import-sort-style-module": "^6.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-import-sort": "^0.0.7",
		"typescript": "^4.6.4"
	},
	"dependencies": {
		"axios": "^0.27.2"
	}
}
